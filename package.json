{
  "name": "nodejs-dashboard-api",
  "version": "1.0.0",
  "description": "API for NodeJS study project",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "prestart": "npm run build",
    "start": "node dist/main.js",
    "dev": "nodemon",
    "dev:inspect": "nodemon -e ts,json --exec node --inspect=localhost:9222 -r ts-node/register src/main.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "lint": "eslint ./src/**",
    "lint:fix": "npm run lint -- --fix",
    "build": "tsc",
    "test": "jest --coverage",
    "test:e2e": "jest --config jest.e2e.config.ts --coverage"
  },
  "author": "Pavel Klochkov",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^3.11.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "inversify": "^6.0.1",
    "jsonwebtoken": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "tslog": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.1",
    "prisma": "^3.11.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
